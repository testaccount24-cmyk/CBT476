;;;
;;; Not Common Lisp, but for Macsyma only.
;;;
;;; Not part of standard ZIL since ASCII isn't really ASCII,
;;; if you know what I mean.
;;;

(defun ascii (x)
 (ebcdic x t)
)

(defload ascii zmsascii)

