         TITLE 'NULL - BUILT-IN ZIL 1.3 SUBROUTINE'
***********************************************************************
*                                                                     *
* ZIL Version 1.3 (c) 1989 The Charles Stark Draper Laboratory Inc.   *
*                     All rights reserved.                            *
*                                                                     *
***********************************************************************
         SPACE 1
NULL     #ZBEG MINARGS=1,MAXARGS=1,NAME='NULL'
         SPACE 1
         USING NULL,#15
         SPACE 1
         C     #10,0(,#1)
         BNE   ZXNULLN
         DROP  #15
         LR    #15,#8
         BR    #14
ZXNULLN  LR    #15,#10
         BR    #14
         END
