         SPACE 1
SETSALST #ZBEG MINARGS=1,MAXARGS=1,NAME='SETSALST'
         SPACE 1
         L     #15,0(,#1)          GET ARG 1
         ST    #15,ZLCSALST        STORE INTO SPECIAL ALIST
         BR    #14
         END
