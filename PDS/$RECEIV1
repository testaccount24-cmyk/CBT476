//SBGOLOBT  JOB (ACCT#),S-GOLOB,
// NOTIFY=&SYSUID,
// CLASS=B,MSGCLASS=X
//*----------------------------------------------------------*//
//*--  THIS JOB IS IN MY LOCAL FORMAT.  SBG                --*//
//*--                                                      --*//
//*--  TSO-IN-BATCH RECEIVE JOB TO CREATE PDS'ES FOR LISP  --*//
//*--   (CHANGE USERID AND DATASET NAMING FOR YOUR SHOP)   --*//
//*--                                                      --*//
//*--  NOTE THAT EXTRA PARAMETERS FOR RECEIVE MUST BE ON   --*//
//*--   THE SECOND LINE, OR THEY WON'T WORK ...            --*//
//*--                                                      --*//
//*--  CHANGE SBGOLOB TO YOUR OWN HIGH LEVEL QUALIFIER     --*//
//*--   CHANGE DATA04 TO YOUR VOLUME FOR THE OUTPUT DSN'S  --*//
//*--                                                      --*//
//*----------------------------------------------------------*//
//TSOBATCH EXEC PGM=IKJEFT01
//SYSTSPRT DD SYSOUT=*
//SYSTSIN DD *
 RECEIVE INDS('SBGOLOB.COV458.FILO083(ASM)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.ASM')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(CLIST)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.CLIST')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(CNTL)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.CNTL')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(HELP)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.HELP')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(LISP)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.LISP')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(MACLIB)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.MACLIB')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(MSG)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.MSG')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(OBJ)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.OBJ')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(PANEL)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.PANEL')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(SKEL)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.SKEL')
 RECEIVE INDS('SBGOLOB.COV458.FILO083(TEXT)')
    VOL(DATA04) DSN('SBGOLOB.ZIL.TEXT')
